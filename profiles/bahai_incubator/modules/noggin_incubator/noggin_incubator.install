function noggin_incubator_install() {
  $weight = db_query('SELECT weight FROM {system} WHERE type = ? AND name = ?', array('module', 'noggin'))->fetchField();
  db_update('system')->condition('name', 'noggin_incubator')->fields(array('weight' => ($weight + 1)))->execute();
}
